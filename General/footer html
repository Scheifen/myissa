    };
    if (window.location.href.split(".comp", 1) == "https://online.issa.com/issassa/censsacompanylkup") { $(".header").css({ "display": "none" }) };
    if (window.location.href.search("15010") != "-1" && $("td[headers='ITEM_DESCR']").text() == "GBAC Fundamentals Online Course") { simpCheckout(); document.cookie="product_id=GBAC Fundamentals Online Course"; };
