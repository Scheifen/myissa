<script type="text/javascript">
  
function chartity_text() {
    $(document).ready(function() {
        if ($(".t-Report-cell[headers=ITEM_DESCR]").text() != "GBAC Fundamentals Online Course") {
            $('#aaEcShoppingCartCustomDonateHdr').replaceWith("ISSA Charities aspires to be the Cleaning Industryâ€™s very own charitable and philanthropic vehicle. Through our charitable programs, ISSA Charities works to advance women, diversity, equity, and inclusion in the cleaning industry, provides professional home cleaning for cancer patients, and offers scholarships to deserving students and interns.</br></br><a href='https://issacharities.org/'' target='_blank'>Learn more about ISSA charities</a>")
        };
        if ($(".t-Report-cell[headers=ITEM_DESCR]").text() === "GBAC Fundamentals Online Course") {
            $("#aaECFundOptions").remove()
        };
    })
};

$(document).ready(chartity_text());
</script>
<script>
x = $("#CB3").text().split(",");

var arrayLength = x.length;

function rmCBEinv2() {
    if (window.location.pathname == "/issassa/arssainvpay.pay_page" && arrayLength > 1) {
        for (var i = 0; i < arrayLength; i++) {
            $("input[value=" + x[i] + "]").parent().parent().remove()
        };
    };
};

$(window).load(rmCBEinv2());
