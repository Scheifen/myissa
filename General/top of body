function setCookie(name, value, days) {
    var d = new Date; 
    var domo = window.location.hostname.match(/[^\s.]+\.[^\s.]+$/)[0];
    d.setTime(d.getTime() + 24*60*60*1000*days);
    document.cookie = name + "=" + value + ";path=/;expires=" + d.toGMTString() + ";domain=" + domo;
}
 
var employer_id = document.getElementById("employer_id").innerHTML;
var customer_id = document.getElementById("customer_id").innerHTML;  
var company_name = document.getElementById("company_name").innerHTML;
var company_type = document.getElementById("company_type").innerHTML;
