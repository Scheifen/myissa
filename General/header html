<script>

  function setCookie(name, value, days) {
    var d = new Date; 
    var domo = window.location.hostname.match(/[^\s.]+\.[^\s.]+$/)[0];
    d.setTime(d.getTime() + 24*60*60*1000*days);
    document.cookie = name + "=" + value + ";path=/;expires=" + d.toGMTString() + ";domain=" + domo;
}


function loggedout(){
setCookie("loggedout","yes",365);
window.location.href = "https://login.issa.com/account/logout";

}

setCookie("loggedout","",0)

function getCookieValue(a) {
    var b = document.cookie.match('(^|[^;]+)\\s*' + a + '\\s*=\\s*([^;]+)');
    return b ? b.pop() : '';
}


var ssalanguage = getCookieValue("ssalanguage");

if (ssalanguage == 'korean'){
$('.going-back a').attr("href", "https://korea.issa.com/");
$('.going-back-logout a').attr("href", "https://online.issa.com/issassa/ssalogout?p_url=https://korea.issa.com/");
}
if (ssalanguage == 'spanish'){
$('.going-back a').attr("href", "https://latam.issa.com/");
$('.going-back-logout a').attr("href", "https://online.issa.com/issassa/ssalogout?p_url=https://latam.issa.com/");
}



$( document ).ready(function() {
if (company_type == "Residential Cleaner"){
$(".logo-identity").attr("src","https://go.issa.com/wp-content/uploads/2019/09/myARCSI-logo-2019.png");
$(".branding-name").text("myARCSI");

}
if (subclass == "IEHA" || subclass == "IEHACHAP" || subclass == "IEHAFRON" || subclass == "IEHAGRP" || subclass == "IEHARET"){
$(".logo-identity").attr("src","https://go.issa.com/wp-content/uploads/2019/09/myIEHA-logo-2019.png");
$(".branding-name").text("myIEHA");
}

});
</script>

