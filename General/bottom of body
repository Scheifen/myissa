if (company_member_type == "RC") {
    $(".arcsi_account").show();
} else {
    $(".arcsi_account").hide();
}


if (company_member_type == "BSC" || company_member_type == "BSCB" || company_member_type == "BSCB-COMP") {

    $(".bsc").show();

}
