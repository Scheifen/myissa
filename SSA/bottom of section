<script>// <![CDATA[
function getCookie(cname) {
  var name = cname + "=";
  var decodedCookie = decodeURIComponent(document.cookie);
  var ca = decodedCookie.split(';');
  for(var i = 0; i <ca.length; i++) {
    var c = ca[i];
    while (c.charAt(0) == ' ') {
      c = c.substring(1);
    }
    if (c.indexOf(name) == 0) {
      return c.substring(name.length, c.length);
    }
  }
  return "";
}

if (employer_id != null && employer_id != '' && employer_id.includes("<!") == false)  {
setCookie("employer_id",employer_id,30)
} else {
	setCookie("employer_id","null",30)
}

if (customer_id != null && customer_id != '' && customer_id.includes("<!") == false){
setCookie("customer_id",customer_id,30)
} else {
	setCookie("customer_id","null",30)
}

if (company_name != null && company_name != '' && company_name.includes("<!") == false){
setCookie("company_name",company_name,30)
} else {
	setCookie("company_name","null",30)
}

if (company_type != null && company_type != '' && company_type.includes("<!") == false){
setCookie("company_type",company_type,30)
} else {
	setCookie("company_type","null",30)
}
  
if (user_job != null && user_job != '' && user_job.includes("<!") == false){
setCookie("user_job",user_job,30)
} else {
	setCookie("user_job","null",30)
}

