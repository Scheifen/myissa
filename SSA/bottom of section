var cookie_val = getCookie("previousPage");
if (cookie_val != null && cookie_val != ''){
document.cookie = "previousPage= ; expires = Thu, 01 Jan 1970 00:00:00 GMT; path=/; domain=issa.com;";
window.location.href = cookie_val;
}


var xmlhttp = new XMLHttpRequest();
        var xmlhttp2 = new XMLHttpRequest();

        var image_featured = "";
        xmlhttp.onreadystatechange = function () {
            if (this.readyState == 4 && this.status == 200) {
                var myObj = JSON.parse(this.responseText);
                document.getElementById("title_news").innerHTML = myObj[0].title.rendered;
                var excerpt = myObj[0].excerpt.rendered;
                document.getElementById("excerpt_news").innerHTML = excerpt.replace(/(<p[^>]+?>|<p>|<\/p>)/img, "");
				
				if(typeof myObj[0]._links['wp:featuredmedia'] != 'undefined' && myObj[0]._links['wp:featuredmedia'] != null){
				image_featured = myObj[0]._links['wp:featuredmedia'][0].href;
				xmlhttp2.open("GET", image_featured, true);
                xmlhttp2.send();
				}else{
					$("#featured_news").remove();
					$(".featured_news").remove();
					$('.news_column').addClass('col-md-12').removeClass('col-md-9');

					
				}

            }
        };
        xmlhttp.open("GET", "https://www.issa.com/wp-json/wp/v2/posts?embed", true);
        xmlhttp.send();

        xmlhttp2.onreadystatechange = function () {
            if (this.readyState == 4 && this.status == 200) {
                var myObj2 = JSON.parse(this.responseText);
                document.getElementById("featured_news").src = myObj2.guid.rendered;

            }
        };
// ]]></script>
